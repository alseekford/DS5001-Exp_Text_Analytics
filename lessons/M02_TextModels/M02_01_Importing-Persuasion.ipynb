   "source": [
    "# Gutenbery typically has a bunch of random text about IP, and policies, etc. so get rid of it\n",
    "#   by regular expression\n",
    "clip_pats = [\n",
    "    r\"\\*\\*\\*\\s*START OF (?:THE|THIS) PROJECT\",\n",
    "    r\"\\*\\*\\*\\s*END OF (?:THE|THIS) PROJECT\"\n",
    "]\n",
    "# backlash s means space\n",
    "# backlash means 0 or 1\n",
    "# (?:x|y) means either can be true to match the string"
   ]
  },
