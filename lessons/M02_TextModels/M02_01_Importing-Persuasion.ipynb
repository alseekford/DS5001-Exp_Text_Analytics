   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "data_home = \"../data\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "text_file = f\"{data_home}/gutenberg/pg105.txt\" #if gutenberg text the rest will work if u just change the file\n",
    "#text_file = \"pg105.txt\"\n",
    "csv_file = f\"{data_home}/output/austen-persuasion.csv\" # The file we will create"
